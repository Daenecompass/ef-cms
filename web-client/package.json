{
  "name": "ef-cms-web-client",
  "version": "1.0.0",
  "description": "Electronic Filing / Case Management System Web Client",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress run",
    "cypress:open": "cypress open --config watchForFileChanges=true",
    "dev": "USTC_ENV=dev parcel --no-cache src/index.pug",
    "dist": "USTC_ENV=prod parcel build --no-cache src/index.pug",
    "lint": "eslint ./src; stylelint ./src/**/*.scss;",
    "test": "nyc mocha --opts ./mocha.opts ./**/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ustaxcourt/ef-cms.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ustaxcourt/ef-cms/issues"
  },
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "extension": [
      "*.js"
    ],
    "exclude": [
      "**/*[tT]est.js"
    ],
    "reporter": [
      "text",
      "text-summary",
      "cobertura",
      "lcov",
      "html"
    ],
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "cache": true
  },
  "homepage": "https://github.com/ustaxcourt/ef-cms#readme",
  "dependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@cerebral/react": "^4.0.1",
    "axios": "^0.18.0",
    "babel-plugin-cerebral": "^1.0.0",
    "cerebral": "^5.0.1",
    "cypress": "^3.1.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.1",
    "nyc": "^13.1.0",
    "page": "^1.10.2",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "pug": "^2.0.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "sass": "^1.14.3",
    "stylelint": "^9.6.0",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "uswds": "^1.6.9"
  }
}
